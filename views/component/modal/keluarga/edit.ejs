<% keluarga.forEach(keluarga => { %>
<div
  class="modal fade"
  id="edit<%= keluarga.id %>"
  tabindex="-1"
  aria-labelledby="editLabel"
  aria-hidden="true"
>
  <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title text-primary" id="editLabel"><i class="bi bi-chevron-right"></i> Edit Keluarga</h5>
        <button
          type="button"
          class="btn-close"
          data-bs-dismiss="modal"
          aria-label="Close"
        ></button>
      </div>
      <div class="modal-body">
        <form method="post" action="/keluarga?_method=put">
            <div class="mb-3">
              <label for="name" class="form-label">Nama Keluarga</label>
              <input type="text" class="form-control" id="name" name="name" placeholder=" " required autocomplete="off" value="<%= keluarga.name %>">
            </div>
            <div class="mb-3">
              <label for="name" class="form-label">Desa Asal</label>
              <input type="text" class="form-control" id="name" name="name" placeholder=" " required autocomplete="off" value="<%= keluarga.origin %>">
            </div>
            <div class="mb-3">
              <label for="criteria" class="form-label">Penghasilan Bulanan</label>
              <select class="form-select" name="criteria" id="criteria" required>
                <option
                <% if (keluarga.criteria[0] === 1) { %>
                 selected
                <% } %>
                value="1">> Rp. 2.000.000</option>
                <option
                <% if (keluarga.criteria[0] === 2) { %>
                 selected
                <% } %>
                value="2">Rp. 1.000.000 - Rp. 2.000.000</option>
                <option
                <% if (keluarga.criteria[0] === 3) { %>
                 selected
                <% } %>
                value="3">Rp. 400.000 - Rp. 1.000.000</option>
                <option
                <% if (keluarga.criteria[0] === 4) { %>
                 selected
                <% } %>
                value="4">Rp. 0 - Rp. 400.000</option>
                <option
                <% if (keluarga.criteria[0] === 5) { %>
                 selected
                <% } %>
                value="5">TIDAK BERPENGHASILAN</option>
              </select>
            </div>
            <div class="mb-3">
              <label for="criteria" class="form-label">Status Tempat Tinggal</label>
              <select class="form-select" name="criteria" id="criteria" required>
                <option
                <% if (keluarga.criteria[1] === 1) { %>
                 selected
                <% } %>
                value="1">MILIK SENDIRI</option>
                <option
                <% if (keluarga.criteria[1] === 2) { %>
                 selected
                <% } %>
                value="2">MILIK ORANG TUA</option>
                <option
                <% if (keluarga.criteria[1] === 3) { %>
                 selected
                <% } %>
                value="3">KONTRAK</option>
                <option
                <% if (keluarga.criteria[1] === 4) { %>
                 selected
                <% } %>
                value="4">KOS</option>
                <option
                <% if (keluarga.criteria[1] === 5) { %>
                 selected
                <% } %>
                value="5">TIDAK ADA</option>
              </select>
            </div>
            <div class="mb-3">
              <label for="criteria" class="form-label">Sumber Mata Pencarian</label>
              <select class="form-select" name="criteria" id="criteria" required>
                <option
                <% if (keluarga.criteria[2] === 1) { %>
                 selected
                <% } %>
                value="1">PNS</option>
                <option
                <% if (keluarga.criteria[2] === 2) { %>
                 selected
                <% } %>
                value="2">KARYAWAN SWASTA</option>
                <option
                <% if (keluarga.criteria[2] === 3) { %>
                 selected
                <% } %>
                value="3">TUKANG</option>
                <option
                <% if (keluarga.criteria[2] === 4) { %>
                 selected
                <% } %>
                value="4">PETANI</option>
                <option
                <% if (keluarga.criteria[2] === 5) { %>
                 selected
                <% } %>
                value="5">PENGANGGURAN</option>
              </select>
            </div>
            <div class="mb-3">
              <label for="criteria" class="form-label">Jumlah Tanggungan</label>
              <select class="form-select" name="criteria" id="criteria" required>
                <option
                <% if (keluarga.criteria[3] === 1) { %>
                 selected
                <% } %>
                value="1">TIDAK ADA</option>
                <option
                <% if (keluarga.criteria[3] === 2) { %>
                 selected
                <% } %>
                value="2">1 ORANG</option>
                <option
                <% if (keluarga.criteria[3] === 3) { %>
                 selected
                <% } %>
                value="3">2 ORANG</option>
                <option
                <% if (keluarga.criteria[3] === 4) { %>
                 selected
                <% } %>
                value="4">3 ORANG</option>
                <option
                <% if (keluarga.criteria[3] === 5) { %>
                 selected
                <% } %>
                value="5">>3 ORANG</option>
              </select>
            </div>
            <div class="mb-3">
              <label for="criteria" class="form-label">Jumlah Keluarga Lansia Atau Sakit Menahun/Kronis</label>
              <select class="form-select" name="criteria" id="criteria" required>
                <option
                <% if (keluarga.criteria[4] === 1) { %>
                 selected
                <% } %>
                value="1">TIDAK ADA</option>
                <option
                <% if (keluarga.criteria[4] === 2) { %>
                 selected
                <% } %>
                value="2">1 ORANG</option>
                <option
                <% if (keluarga.criteria[4] === 3) { %>
                 selected
                <% } %>
                value="3">2 ORANG</option>
                <option
                <% if (keluarga.criteria[4] === 4) { %>
                 selected
                <% } %>
                value="4">3 ORANG</option>
                <option
                <% if (keluarga.criteria[4] === 5) { %>
                 selected
                <% } %>
                value="5">>3 ORANG</option>
              </select>
            </div>
            <div class="mb-3">
              <label for="criteria" class="form-label">Pengeluaran Bulanan</label>
              <select class="form-select" name="criteria" id="criteria" required>
                <option
                <% if (keluarga.criteria[5] === 1) { %>
                 selected
                <% } %>
                value="1">Rp. 0 - Rp. 400.000</option>
                <option
                <% if (keluarga.criteria[5] === 2) { %>
                 selected
                <% } %>
                value="2">Rp. 400.000 - Rp. 1.000.000</option>
                <option
                <% if (keluarga.criteria[5] === 3) { %>
                 selected
                <% } %>
                value="3">Rp. 1.000.000 - Rp. 2.000.000</option>
                <option
                <% if (keluarga.criteria[5] === 4) { %>
                 selected
                <% } %>
                value="4">Rp. 2.000.000 - Rp. 3.000.000</option>
                <option
                <% if (keluarga.criteria[5] === 5) { %>
                 selected
                <% } %>
                value="5">> Rp. 3.000.000</option>
              </select>
            </div>
            <div class="mb-3">
              <label for="criteria" class="form-label">Pendidikan Akhir</label>
              <select class="form-select" name="criteria" id="criteria" required>
                <option
                <% if (keluarga.criteria[6] === 1) { %>
                 selected
                <% } %>
                value="1">SARJANA</option>
                <option
                <% if (keluarga.criteria[6] === 2) { %>
                 selected
                <% } %>
                value="2">SMA</option>
                <option
                <% if (keluarga.criteria[6] === 3) { %>
                 selected
                <% } %>
                value="3">SMP</option>
                <option
                <% if (keluarga.criteria[6] === 4) { %>
                 selected
                <% } %>
                value="4">SD</option>
                <option
                <% if (keluarga.criteria[6] === 5) { %>
                 selected
                <% } %>
                value="5">TIDAK SEKOLAH</option>
              </select>
            </div>

            <input type="hidden" name="id" value="<%= keluarga.id %>" />

        </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-bs-dismiss="modal">Batal</button>
                <button type="submit" class="btn btn-outline-primary"><i class="bi bi-pencil-square"></i> Edit</button>
            </div>
        </form>
    </div>
    </div>
  </div>
</div>
<% }) %>