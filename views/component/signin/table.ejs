<div class="container-fluid">
  <div class="row p-5 justify-content-center">
    <div class="col-12">
      <h1 class="display-6 mb-5 text-center">
        Total Keluarga Terdata : <%= count %>
      </h1>
      <div class="row justify-content-center">
        <% alternativesLabel.forEach((value, index) => { %>
        <div class="col-5 mb-3">
          <table class="table table-striped table-hover text-center">
            <tbody>
              <th colspan="2" class="text-primary">
                <%= alternativesTitle[index] %>
              </th>
              <% value.forEach((element, index2) => { %>
              <tr>
                <td><%= alternativesLabel[index][index2] %></td>
                <td width="50"><%= alternativesCount[index][index2] %></td>
              </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
        <% }) %>
      </div>
    </div>
  </div>
</div>
